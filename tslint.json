{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "interface-over-type-literal": false,
        "no-shadowed-variable": false,
        "no-console": false
    },
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    },
    "jsRules": {
        "object-literal-sort-keys": false
    }
}